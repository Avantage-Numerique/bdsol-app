{
  "name": "bdsol-frontend",
  "version": "0.0.1",
  "description": "BDSOL fronted en nextjs",
  "authors": [
    "Frédéric Rivard",
    "Marc-André Martin",
    "Vincent P. Ruel"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:watch": "next dev",
    "build": "next build",
    "build:debug": "next build --debug",
    "build:staging": "next build && next export",
    "test": "jest --watch",
    "start": "next start -p 3000",
    "docker:dev": "npx concurrently \"npm run build\" \"npm run dev\"",
    "docker:dev:watch": "npx concurrently \"npm run dev:watch\" \"npm run docker:dev:run\"",
    "docker:dev:run": "npx nodemon --legacy-watch",
    "docker:start": "next start -p 3000",
    "docker:test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@popperjs/core": "^2.11.6",
    "@reduxjs/toolkit": "^1.8.1",
    "@shopify/draggable": "^1.0.0-beta.8",
    "blotter": "^1.0.0",
    "bootstrap": "^5.3.0",
    "chroma-js": "^2.4.2",
    "iron-session": "^6.2.1",
    "isomorphic-dompurify": "^0.16.0",
    "jest-environment-jsdom": "^28.1.0",
    "line-awesome": "^1.3.0",
    "moment": "^2.29.4",
    "next": "^12.3.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.8.2",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.1.0",
    "react-popper": "^2.3.0",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.1",
    "react-scroll": "^1.8.7",
    "react-select": "^5.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.39.0",
    "jest": "^28.1.0",
    "purgecss": "^5.0.0",
    "sass": "^1.63.2"
  }
}
