{
  "name": "bdsol-frontend",
  "version": "0.0.1",
  "description": "BDSOL fronted en nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:watch": "next dev",
    "build": "next build",
    "test": "jest --watch",
    "start": "next start -p ${PORT:=3000}",
    "docker:dev": "npx concurrently \"npm run build\" \"npm run dev\"",
    "docker:dev:watch": "npx concurrently \"npm run dev:watch\" \"npm run docker:dev:run\"",
    "docker:dev:run": "npx nodemon --legacy-watch",
    "docker:start": "next start -p ${PORT:=3000}",
    "docker:test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "blotter": "^1.0.0",
    "isomorphic-dompurify": "^0.16.0",
    "jest-environment-jsdom": "^28.1.0",
    "next": "^12.1.5",
    "quill": "^1.3.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-quilljs": "^1.2.17",
    "react-redux": "^8.0.1",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "concurrently": "^7.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15"
  }
}
